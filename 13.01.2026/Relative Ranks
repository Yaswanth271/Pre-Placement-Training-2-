class Solution:
    def findRelativeRanks(self, score):
        n = len(score)
        result = [""] * n

        # Pair scores with original indices
        athletes = sorted(
            [(s, i) for i, s in enumerate(score)],
            reverse=True
        )

        for rank, (_, idx) in enumerate(athletes, start=1):
            if rank == 1:
                result[idx] = "Gold Medal"
            elif rank == 2:
                result[idx] = "Silver Medal"
            elif rank == 3:
                result[idx] = "Bronze Medal"
            else:
                result[idx] = str(rank)

        return result
