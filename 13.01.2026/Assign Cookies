class Solution(object):
    def findContentChildren(self, g, s):
        """
        :type g: List[int]
        :type s: List[int]
        :rtype: int
        """
        g.sort()
        s.sort()
        count=0

        n=len(g)-1
        m=len(s)-1

        while n>=0 and m>=0:
            if s[m]>=g[n]:
                count+=1
                m-=1
                n-=1
            else:
                n-=1
        return count 
