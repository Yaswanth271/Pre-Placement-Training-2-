class Solution(object):
    def generate(self, numRows):
        """
        :type numRows: int
        :rtype: List[List[int]]
        """
        res = [[1]]

        for i in range(2, numRows+1):
            prev = res[-1]
            curr = [1] + [prev[j] + prev[j - 1] for j in range(1, len(prev))] + [1]
            
            res.append(curr)


        return res
