class Solution(object):
    def topKFrequent(self, nums, k):
        """
        :type nums: List[int]
        :type k: int
        :rtype: List[int]
        """
        ans = []
        occurrence = Counter(nums)

        nums = sorted(occurrence, key=occurrence.get, reverse=True)

        for i in range(k):
            ans.append(nums[i])
        return ans
        
